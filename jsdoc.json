{
    "plugins": [
        "plugins/markdown"
    ],
    "markdown": {
        "idInHeadings": true // This is important for clean-jsdoc-theme, otherwise some features might not work.
    },
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "theme_opts": {
            "default_theme": "light",
            "homepageTitle": "EnIndex Documentation",
            "menu": [
                {
                    "title": "Doc Home",
                    "link": "index.html",
                    "target": "_self"
                },
                {
                    "title": "Project Website",
                    "link": "https://github.com/desiic/enindex",
                    "target": "_blank"
                }
            ],
            "sections": ["Modules"],
            "include_css": ["./jsdoc-custom.css"]
        }
    }
}